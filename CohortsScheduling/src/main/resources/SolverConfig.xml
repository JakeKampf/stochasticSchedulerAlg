<?xml version="1.0" encoding="UTF-8"?>
<solver>
  <scanAnnotatedClasses/>
	<scoreDirectorFactory>
    	<easyScoreCalculatorClass>CohortScoring.cohortScoring</easyScoreCalculatorClass>
  	</scoreDirectorFactory>
  <termination>
    <secondsSpentLimit>300</secondsSpentLimit>
  </termination>
  <randomSeed>1234</randomSeed>
  <localSearch>
  	<changeMoveSelector/>
  	<termination><!-- Solver termination -->
      <secondsSpentLimit>290</secondsSpentLimit>
  	</termination>
  	<acceptor>
  		<simulatedAnnealingStartingTemperature>20hard/100soft</simulatedAnnealingStartingTemperature>
  	</acceptor>
  	<forager>
  		<acceptedCountLimit>4</acceptedCountLimit>
  	</forager>
  </localSearch>
  
</solver>
